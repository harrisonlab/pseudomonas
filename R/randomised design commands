trt<-c("R1-5244","R1-5300","R1-9646","R1-5270","R2-5255","R2-LEAF","R2-SC214","Pss9644", "Pss9656","Pss7928A","avii5271","control")
outdesign <-design.rcbd(trt, 4, serie=2,4,"Wichmann-Hill") # seed = 4
print(outdesign$sketch)

